{
  "containerDefinitions": [
    {
      "name": "tis-generic-upload",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/generic-upload:latest",
      "essential": true,
      "secrets": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "valueFrom": "/tis/generic-upload/preprod/spring-profiles-active"
        },
        {
          "name": "AUTH_HOST",
          "valueFrom": "/tis/generic-upload/preprod/auth/host"
        },
        {
          "name": "AUTH_PORT",
          "valueFrom": "/tis/generic-upload/preprod/auth/port"
        },
        {
          "name": "DBHOST",
          "valueFrom": "/tis/generic-upload/preprod/db/host"
        },
        {
          "name": "DBPORT",
          "valueFrom": "/tis/generic-upload/preprod/db/port"
        },
        {
          "name": "DBNAME",
          "valueFrom": "/tis/generic-upload/preprod/db/name"
        },
        {
          "name": "DBUSER",
          "valueFrom": "/tis/generic-upload/preprod/db/user"
        },
        {
          "name": "DBPASSWORD",
          "valueFrom": "/tis/generic-upload/preprod/db/password"
        },
        {
          "name": "USE_SSL",
          "valueFrom": "/tis/generic-upload/preprod/db/ssl"
        },
        {
          "name": "LOG_DIR",
          "valueFrom": "/tis/generic-upload/preprod/log/dir"
        },
        {
          "name": "LOG_LEVEL",
          "valueFrom": "/tis/generic-upload/preprod/log/level"
        },
        {
          "name": "GENERIC_UPLOAD_AWS_BUCKET_NAME",
          "valueFrom": "/tis/generic-upload/preprod/s3/bucket-name"
        },
        {
          "name": "DEADLETER_DIR",
          "valueFrom": "/tis/generic-upload/preprod/deadletter/dir"
        },
        {
          "name": "KC_REALM",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/realm"
        },
        {
          "name": "KC_MASTER_REALM",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/master-realm"
        },
        {
          "name": "KC_CLIENT_ID",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/client-id"
        },
        {
          "name": "KC_SERVER_URL",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/server-url"
        },
        {
          "name": "KC_USERNAME",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/username"
        },
        {
          "name": "KC_PASSWORD",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/password"
        },
        {
          "name": "KC_TIMEOUT",
          "valueFrom": "/tis/generic-upload/preprod/keycloak/timeout"
        },
        {
          "name": "PROFILE_HOST",
          "valueFrom": "/tis/generic-upload/preprod/profile/host"
        },
        {
          "name": "PROFILE_PORT",
          "valueFrom": "/tis/generic-upload/preprod/profile/port"
        },
        {
          "name": "TCS_SERVER_URL",
          "valueFrom": "/tis/generic-upload/preprod/tcs/server-url"
        },
        {
          "name": "REFERENCE_SERVER_URL",
          "valueFrom": "/tis/generic-upload/preprod/reference/server-url"
        },
        {
          "name": "ASSESSMENTS_SERVER_URL",
          "valueFrom": "/tis/generic-upload/preprod/assessments/server-url"
        },
        {
          "name": "SERVICE_BUS_CONNECTION_STRING",
          "valueFrom": "/tis/generic-upload/preprod/SERVICE_BUS_CONNECTION_STRING"
        },
        {
          "name": "SERVICE_BUS_QUEUE_NAME",
          "valueFrom": "/tis/generic-upload/preprod/SERVICE_BUS_QUEUE_NAME"
        },
        {
          "name": "SERVICE_BUS_Q_RECEIVE_MODE",
          "valueFrom": "/tis/generic-upload/preprod/SERVICE_BUS_Q_RECEIVE_MODE"
        },
        {
          "name": "SERVICE_BUS_TOPIC_NAME",
          "valueFrom": "/tis/generic-upload/preprod/SERVICE_BUS_TOPIC_NAME"
        },
        {
          "name": "SERVICE_BUS_SUBSCRIPTION_NAME",
          "valueFrom": "/tis/generic-upload/preprod/SERVICE_BUS_SUBSCRIPTION_NAME"
        },
        {
          "name": "SERVICE_BUS_S_RECEIVE_MODE",
          "valueFrom": "/tis/generic-upload/preprod/SERVICE_BUS_S_RECEIVE_MODE"
        },
        {
          "name": "CLOUD_BLOB_ACCOUNT_NAME",
          "valueFrom": "/tis/generic-upload/preprod/CLOUD_BLOB_ACCOUNT_NAME"
        },
        {
          "name": "CLOUD_BLOB_ACCOUNT_KEY",
          "valueFrom": "/tis/generic-upload/preprod/CLOUD_BLOB_ACCOUNT_KEY"
        },
        {
          "name": "CLOUD_BLOB_CONTAINER_NAME",
          "valueFrom": "/tis/generic-upload/preprod/CLOUD_BLOB_CONTAINER_NAME"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-preprod-tis-generic-upload",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "awslogs-tis-generic-upload",
          "awslogs-create-group": "true"
        }
      },
      "portMappings": [
        {
          "containerPort": 8099
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "tis-generic-upload"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        },
        {
          "name": "SENTRY_ENVIRONMENT",
          "value": "stage"
        },
        {
          "name": "PROFILE_JWT_CACHE_TTL",
          "value": "10"
        }
      ]
    }
  ],
  "family": "tis-generic-upload-preprod",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "2048"
}
